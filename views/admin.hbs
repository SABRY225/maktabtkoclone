    <div class="overlay_2"></div>
    <!--header section start -->
    <div class="header_section-2 header_admin">
        <div class="header_bg">
            <div class="container">
                <nav class="navbar navbar-dark bg-dark  fixed-top">
                    <div class="container-fluid">
                        <a class="navbar-brand text-start" href="#" style="  color: #ffc107;">مكتبتكو</a>
                        <img src="/images/logo (3).png" width="50px">
