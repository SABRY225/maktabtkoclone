        .home {
            position: absolute;
            left: 45%;
            top: 10rem;
        }

        .loading {
            width: 124px;
            height: 124px;
        }

        .loading svg {
            display: block;
            width: 100%;
            height: 100%;
        }

        .circle {
            transform: rotate(-90deg);
            transform-origin: center;
            stroke-dasharray: 380;
            stroke-dashoffset: 380;
            animation: circle_4 2s ease-in-out forwards;
        }

        .check {
            stroke-dasharray: 45;
            stroke-dashoffset: 45;
            animation: check_4 0.2s 2s ease-in-out forwards;
        }

        @keyframes circle_4 {
            0% {
                stroke-dashoffset: 380;
            }

            100% {
                stroke-dashoffset: 0;
            }
        }

        @keyframes check_4 {
            0% {
                stroke-dashoffset: 45;
            }

            100% {
                stroke-dashoffset: 90;
            }
        }

        .btn_main {
            width: 100%;
            float: left;
            padding-top: 40px;
            margin-left: 20px;
        }

        .text_done {
